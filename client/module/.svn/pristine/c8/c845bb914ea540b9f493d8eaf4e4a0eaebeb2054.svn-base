        "parent_id": "533f16a1-94c4-4ad4-a9c6-39bf0af91b9d",
        "properties": "{\"name_model\":\"schema\",\"label\":\"Tax summary compatation\",\"on_change_field_model\":\"schema\",\"mapped_field_model\":\"schema\",\"filter_field_model\":\"schema\",\"filter_by_fields_model\":\"schema\",\"javascript\":\"\\r\\n$('#__FORM_ID__')\\r\\n        .find('[name *= \\\"[product_id]\\\"]')\\r\\n        .change(function(event) {\\r\\n            var href = 'product\\\/products\\\/view.json';\\r\\n            var row = $(this).closest('.last-data-row'); \\r\\n            var tokeninput = row.find('.tokeninput-popup-autocomplete');\\r\\n            $.getJSON(CONFIG['base'] + href, {\\\"id\\\": $(this).val()}, function(response) {\\r\\n                var line_taxes = [];\\r\\n                var taxes = [];\\r\\n                if ($.isset(response['data']) && $.isset(response['data']['products']) && $.isset(response['data']['products']['line_taxes'])) {\\r\\n                    var taxes = response['data']['products']['line_taxes'];\\r\\n                }\\r\\n                row.find('[name *= \\\"[line_taxes]\\\" ]')\\r\\n                        .filter('[name *=\\\"[tax_id]\\\"]')\\r\\n                        .each(function() {\\r\\n                            container = $(this).parents(':first');\\r\\n                            if (container.find('[name *=\\\"[deleted]\\\"]').val() != 1) {\\r\\n                                line_taxes.push(container);\\r\\n                            }\\r\\n                        });\\r\\n                var toBeDeleted = line_taxes.splice(taxes.length);\\r\\n                $.each(line_taxes, function(k, container) {\\r\\n                    tax = taxes.shift();\\r\\n                    container.find('[name *=\\\"[tax_id]\\\"]').val(v['tax_id']);\\r\\n                    container.find('[name *=\\\"[__tax_id]\\\"]').val(v['__tax_id']);\\r\\n                    container.find('[name *=\\\"[tax_id_model]\\\"]').val(v['tax_id_model']);\\r\\n                });\\r\\n                $.each(taxes, function(k, tax) {\\r\\n                    tokeninput.tokenInput(\\\"add\\\", {'key': tax['tax_id'], 'name': tax['__tax_id'], 'model': 'taxes'})\\r\\n                });\\r\\n                $.each(toBeDeleted, function(k, container) {\\r\\n                    container.find('.token-input-delete-token').trigger('click');\\r\\n                });\\r\\n                row.find('[name *=\\\"[line_taxes]\\\" ]')\\r\\n                        .filter('[name *=\\\"[tax_id]\\\"]')\\r\\n                        .each(function() {\\r\\n                            if ($(this).parents(':first').find('[name *=\\\"[deleted]\\\"]').val() != 1) {\\r\\n                                $(this).trigger('change');\\r\\n                            }\\r\\n                        });\\r\\n            });\\r\\n        });\\r\\n\\r\\n\\r\\n$('#__FORM_ID__')\\r\\n.find('.tokeninput-popup-autocomplete')\\r\\n.bind('change',function(event){\\r\\n    var form=$('#__FORM_ID__');\\r\\n    \\\/\\\/ read tax information\\r\\n    var taxes=[];\\r\\n    var taxesElements={};\\r\\n    form\\r\\n    .find('[ name *= \\\"[line_taxes]\\\" ]')\\r\\n    .filter('[name *=\\\"[tax_id]\\\"]')\\r\\n    .each(function(){\\r\\n        var taxID=$(this).val();\\r\\n        var container=$(this).parents(':first');\\r\\n        var taxType=container.find('[name *= \\\"[tax_type]\\\" ]').val();\\r\\n         if(taxID !=\\\"\\\" && taxType==\\\"\\\"  ){\\r\\n            taxes.push(taxID);\\r\\n            taxesElements[\\\"tax_\\\"+taxID]=container;\\r\\n         }\\r\\n    });\\r\\n    if(taxes.length > 0){\\r\\n        var q={}; \\r\\n         q['fields']=['taxes.*','tax_types.name AS tax_type'];\\r\\n         q['where']={'{{MODEL}}.{{PRIMARY_KEY}}':taxes};\\r\\n         q['method']='find';\\r\\n         var href=CONFIG['base']+'accounting\\\/taxes\\\/index.json';\\r\\n         $.getJSON(href,{'q':encodeURIComponent(JSON.stringify(q))},function(response){\\r\\n              if(\\r\\n              typeof(response['paginate']) != 'undefined' \\r\\n              && typeof(response['paginate']['data']) !='undefined' ){\\r\\n                    $.each(response['paginate']['data'],function(k,v){\\r\\n                        if(typeof(taxesElements[\\\"tax_\\\"+v.id]) != 'undefined'){\\r\\n                            taxesElements[\\\"tax_\\\"+v.id].find('[name *= \\\"[tax_type]\\\" ]').val(v.tax_type);\\r\\n                            taxesElements[\\\"tax_\\\"+v.id].find('[name *= \\\"[tax_value]\\\" ]').val(v.amount);\\r\\n                        }\\r\\n                    }); \\r\\n              }\\r\\n              computeTaxTotals(form);\\r\\n         }); \\r\\n    }else{\\r\\n    \\\/\\\/ compute individual taxes & compute summary taxes\\r\\n        computeTaxTotals(form);\\r\\n    }\\r\\n    event.stopImmediatePropagation();\\r\\n    event.stopPropagation();\\r\\n    return false;\\r\\n\\r\\n});\\r\\n\\r\\nfunction computeTaxTotals(form){\\r\\n   var computeTaxTotals=form.attr('computeTaxTotals');\\r\\n   if(typeof(computeTaxTotals) != 'undefined' && computeTaxTotals ==1 ){\\r\\n      return;\\r\\n   }\\r\\n   form.attr('computeTaxTotals',1);\\r\\n    var taxes={};\\r\\n    var taxID;\\r\\n    var taxType;\\r\\n    var taxValue;\\r\\n    var container;\\r\\n    var totalAmount=0;\\r\\n    var taxAmount;\\r\\n    var totalTaxAmount=0;\\r\\n    form\\r\\n    .find('[ name *= \\\"[line_taxes]\\\" ]')\\r\\n    .filter('[name *=\\\"[tax_id]\\\"]')\\r\\n    .each(function(){\\r\\n         taxID=$(this).val();    \\r\\n         if($.trim(taxID) !=\\\"\\\" ){\\r\\n            container=$(this).parents(':first');\\r\\n            if(container.find('[name *=\\\"[deleted]\\\"]').val() != 1){\\r\\n                if(typeof(taxes[\\\"tax_\\\"+taxID]) == \\\"undefined\\\" ){\\r\\n                    var taxLabel=$(this).next().val();\\r\\n                    taxes[\\\"tax_\\\"+taxID]={\\\"__value\\\":taxLabel, \\\"value\\\":taxID,\\\"tax_amount\\\":0};\\r\\n                }\\r\\n                totalAmount= $.parseFloat($(this).closest('.last-data-row')\\r\\n                         .find('[name *=\\\"[total_amount]\\\"]').val());\\r\\n            \\r\\n                taxValue= $.parseFloat(container.find('[name *=\\\"[tax_value]\\\"]').val());\\r\\n                taxType=container.find('[name *=\\\"[tax_type]\\\"]').val();\\r\\n                taxAmount=(taxType==\\\"Fixed Amount\\\"?taxValue:(totalAmount\\\/100)*taxValue);\\r\\n                container.find('[name *=\\\"[tax_amount]\\\"]').val($.format(taxAmount,'n2'));\\r\\n                taxes[\\\"tax_\\\"+taxID][\\\"tax_amount\\\"] += taxAmount;\\r\\n                totalTaxAmount += taxAmount;\\r\\n            }\\r\\n         }  \\r\\n    });\\r\\n    var grid=form.find('[__name=\\\"taxes\\\"]').attr('trigger_change',\\\"0\\\");\\r\\n    var currentTaxes={};\\r\\n    grid.find('[name *=\\\"[tax_id]\\\"]').each(function(){\\r\\n        var taxID=$(this).val();    \\r\\n         if($.trim(taxID) !=\\\"\\\"){\\r\\n           if(typeof(taxes[\\\"tax_\\\"+taxID]) == 'undefined'){\\r\\n              $(this).closest(\\\".last-data-row\\\").trigger('grid_row_delete');\\r\\n           }\\r\\n           if($(this).closest(\\\".grid-template\\\").find('[name *=\\\"[deleted]\\\"]').val() != 1){\\r\\n                currentTaxes[\\\"tax_\\\"+taxID]=$(this).closest(\\\".last-data-row\\\");\\r\\n           }\\r\\n         }\\r\\n    });\\r\\n    \\r\\n   $.each(taxes,function(k,v){\\r\\n      var row;\\r\\n      if(typeof(currentTaxes[k]) =='undefined' ){\\r\\n        grid.trigger('grid_row_add');\\r\\n        row=grid.find('.last-data-row:'+(CONFIG.is_mobile ==1?'first':'last'));\\r\\n      }else{\\r\\n        row=currentTaxes[k];\\r\\n      }\\r\\n        row.find('[name*=\\\"[tax_id]\\\"]').val(v.value);\\r\\n        row.find('[name*=\\\"[__tax_id]\\\"]').val(v.__value);\\r\\n        row.find('[name*=\\\"[tax_amount]\\\"]').val($.format(v.tax_amount,'n2'));\\r\\n    });\\r\\n    \\\/\\\/ mark deleted summary records no more in use\\r\\n     $.each(currentTaxes,function(k,v){\\r\\n        if(typeof(taxes[k]) =='undefined' ){\\r\\n           v.trigger('grid_row_delete');\\r\\n        }\\r\\n     });\\r\\n     \\r\\n     \\r\\n     \\r\\n    totalTaxAmount=form.find('[name*=\\\"[sub_total]\\\"]').value()\\r\\n                    +form.find('[name *=\\\"[shipping_cost]\\\"]').value()\\r\\n                    +totalTaxAmount+0.00;\\r\\n    form.find('[name*=\\\"[total_after_discount]\\\"]').val($.format(totalTaxAmount,'n2'));\\r\\n    form.attr('computeTaxTotals',0);\\r\\n}\\r\\n\\r\\n\\r\\n$('#__FORM_ID__')\\r\\n.find('[name*=\\\"[total_amount]\\\"]')\\r\\n.bind('change',function(event){\\r\\n     computeTaxTotals($('#__FORM_ID__'));\\r\\n     event.stopImmediatePropagation();\\r\\n     event.stopPropagation();\\r\\n     return false;     \\r\\n});\\r\\n\\r\\n\\r\\n$('#__FORM_ID__')\\r\\n.find('[name*=\\\"[sub_total]\\\"]')\\r\\n.bind('change',function(event){\\r\\n   computeTaxTotals($('#__FORM_ID__'));\\r\\n});\\r\\n\\r\\n$('#__FORM_ID__')\\r\\n.find('[name*=\\\"[shipping_cost]\\\"]')\\r\\n.bind('change',function(event){\\r\\n   computeTaxTotals($('#__FORM_ID__'));\\r\\n});\\r\\n\",\"require_document_ready\":\"1\",\"helper\":\"\\\\tushar\\\\form\",\"method\":\"javascript\"}",
        "template": "javascript"

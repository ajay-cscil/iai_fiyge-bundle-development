        "then_condition": "if(isset($object->data[\"routing_order_id\"])){\r\n    $routingOrderID=\\select(\"routing_order_id\")\r\n                     ->from($object)\r\n                     ->where(\"id\",$object->id)\r\n                     ->execute()\r\n                     ->fetch(\\PDO::FETCH_COLUMN);\r\n    $workOrderObj=$object->{'work_orders'};\r\n    $workOrderCount=\\select(\"id\")\r\n    ->from($workOrderObj)\r\n    ->inserted()\r\n    ->where(\"manufacturing_order_id\",$object->id)\r\n    ->execute()\r\n    ->fetch(\\PDO::FETCH_COLUMN);\r\n    $routingOrderObj=$object->{'routing_orders'};\r\n    \r\n    \r\n    \r\n    if(empty($workOrderCount) || ($routingOrderID != $object->data[\"routing_order_id\"])  ){\r\n        $routingOrder=current($routingOrderObj->read($object->data[\"routing_order_id\"]));\r\n        if(!empty($routingOrder['work_center_operations'])){\r\n            foreach($routingOrder['work_center_operations'] as $centerOperations){\r\n                $centerOperations['manufacturing_order_id']=$object->id;\r\n            \t$centerOperations['name'] = $routingOrder['name'].':'.$centerOperations['name'];\r\n    \t\t\t$workOrder=array_diff_key($centerOperations,array('id'=>1));\r\n    \t\t\t$workOrderObj->save($workOrder,array('atomic'=>true,'validate'=>false)); \r\n    \t\t}\r\n    \t}\r\n        \r\n    }\r\n}"
